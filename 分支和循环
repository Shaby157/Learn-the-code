#define _CRT_SECURE_NO_WARNINGS 1

#include <stdio.h>
#include<string.h>
int main()
{
	int age = 100;
	if (age < 18)
	{
		printf("未成年\n");
		printf("不允许谈恋爱\n");
	}
	else if (age >= 18 && age < 28)
		printf("青年\n");
	else if (age >= 28 && age < 50)
		printf("壮年\n");
	else if (age >= 50 && age < 90)
		printf("老年\n");
	else
		printf("老不死\n");
	return 0;
}


int main()
{
	int a = 1;
	int b = 2;
	if (a == 0)
		if (b == 2)
	printf("haha\n");
		else
			printf("hehe\n");
	return 0;
}


int main()
{
	int i = 1;
	printf("0-100奇数为");
	while (i <= 100)
	{
		if (i % 2 == 1);
		printf("%d  ", i);
		i++;
	}
	return 0;
}


int main()
{
	int a = 8;
	int num = 0;
	num = a % 2;
	printf("num=%d\n", num);
	if (num == 0)
		printf("a是偶数");
	else
		printf("a是奇数");
	return 0;
}


int main()
{
	int day = 0;
	scanf("%d", &day);
	switch (day)
	{
	case 1:
		printf("星期一\n");
		break;
	case 2:
		printf("星期二\n");
		break;
	case 3:
		printf("星期三\n");
		break;
	case 4:
		printf("星期四\n");
		break;
	case 5:
		printf("星期五\n"); 
		break;
	case 6:
		printf("星期六\n");
		break;
	case 7:
		printf("星期天\n");
		break;
	}
	return 0;
}


int main()
{
	int i = 1;
	while (i<=10)
	{
		printf("%d  ", i);
		i++;
	}
	return 0;
}


int main()
{
	printf("请选择你要的点数\n");
	int day = 0;
	scanf("%d", &day);
	
	switch (day)
	{
			case 1:
				printf("向前走一步\n");
				break;
			case 2:
				printf("倒退一步\n");
				break;
			case 3:
				printf("向前两步\n");
				break;
			case 4:
				printf("向前三步\n");
				break;
			case 5:
				printf("重新再选一个\n");
				break;
			case 6:
				printf("赢得game\n");
				break;
			default:
				printf("不要太贪心了，重新输入\n");
	}
	return 0;
}


int main()
{
	int ch = 0;
	char password[10] = {0};
	printf("请输入密码: ");
	scanf("%s",password);	
	getchar();
	printf("确认密码(Y/N): ");
	ch = getchar();
		if (ch == 'Y')
			printf("确认成功\n");
		else
			printf("确认失败\n");

	return 0;
}
